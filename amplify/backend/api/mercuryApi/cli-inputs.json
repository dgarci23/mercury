{
  "version": 1,
  "paths": {
    "/user/{userId}": {
      "name": "/user/{userId}",
      "lambdaFunction": "mercuryApiLambda",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/user/address/{userId}": {
      "name": "/user/address/{userId}",
      "lambdaFunction": "mercuryApiLambda",
      "permissions": {
        "setting": "open"
      }
    }
  }
}